{
  "name": "lapriere-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm-run-all build:license build:astro",
    "build:license": "license-checker-rseidelsohn --plainVertical --production --out ./public/LICENSES.txt",
    "build:astro": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "npm-run-all --parallel check:eslint check:stylelint check:prettier check:astro check:tsc",
    "check:tsc": "tsc --noEmit",
    "check:astro": "astro check",
    "check:eslint": "eslint --cache --cache-location node_modules .",
    "check:stylelint": "stylelint '**/*.{css,scss,astro}'",
    "check:prettier": "prettier --check --cache . '**/*.{css,scss,astro}'",
    "fix": "npm-run-all fix:eslint fix:stylelint fix:prettier",
    "fix:eslint": "eslint --cache --cache-location node_modules --fix .",
    "fix:stylelint": "stylelint --fix '**/*.{css,scss,astro}'",
    "fix:prettier": "prettier --write --cache . '**/*.{css,scss,astro}'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^9.0.0",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.5.2",
    "@builder.io/qwik": "^1.4.5",
    "@iconify-json/devicon": "^1.1.35",
    "@iconify-json/material-symbols": "^1.1.73",
    "@iconify-json/material-symbols-light": "^1.1.13",
    "@iconify/tailwind": "^0.1.4",
    "@qwikdev/astro": "^0.5.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgx/core": "^1.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/fs-extra": "^11.0.4",
    "astro": "^4.3.7",
    "clsx": "^2.1.0",
    "cva": "beta",
    "daisyui": "^4.7.2",
    "htmlrewriter": "^0.0.7",
    "js-cookie": "^3.0.5",
    "license-checker-rseidelsohn": "^4.3.0",
    "npm-run-all2": "^6.1.2",
    "rehype-external-links": "^3.0.0",
    "remark-link-card": "git@github.com:totto2727-org/remark-link-card.git",
    "remeda": "^1.43.0",
    "sass": "^1.71.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "unplugin-icons": "^0.18.5"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.4",
    "@eslint/js": "^8.56.0",
    "@types/js-cookie": "^3.0.6",
    "bun-types": "latest",
    "eslint": "9.0.0-beta.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.1",
    "wrangler": "^3.28.2"
  }
}
