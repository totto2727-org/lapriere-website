{
  "name": "lapriere-website",
  "type": "module",
  "version": "0.0.1",
  "imports": {
    "#js/*": {
      "development": "./src/*.development.js",
      "default": "./src/*.js"
    }
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm-run-all build:license build:astro",
    "build:license": "license-checker-rseidelsohn --plainVertical --production --out ./public/LICENSES.txt",
    "build:astro": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "npm-run-all --parallel check:stylelint check:eslint check:astro check:tsc",
    "check:tsc": "tsc --noEmit",
    "check:astro": "astro check",
    "check:stylelint": "stylelint '**/*.{html,astro,css,scss}'",
    "check:eslint": "eslint .",
    "fix": "npm-run-all fix:stylelint fix:eslint",
    "fix:stylelint": "stylelint --fix '**/*.{html,astro,css,scss}'",
    "fix:eslint": "eslint --fix .",
    "update:npm": "ncu --removeRange -x @totto/*",
    "update:jsr": "ncu --removeRange @totto/* -r https://npm.jsr.io/"
  },
