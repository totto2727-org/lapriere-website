{
  "name": "lapriere-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm-run-all build:license build:astro",
    "build:license": "license-checker-rseidelsohn --plainVertical --production --out ./public/LICENSES.txt",
    "build:astro": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "npm-run-all --parallel check:eslint check:stylelint check:prettier check:astro check:tsc",
    "check:tsc": "tsc --noEmit",
    "check:astro": "astro check --minimumSeverity warning",
    "check:eslint": "eslint --cache --cache-location node_modules .",
    "check:stylelint": "stylelint '**/*.{css,scss,astro}'",
    "check:prettier": "prettier --check --cache . '**/*.{css,scss,astro}'",
    "fix": "npm-run-all fix:eslint fix:stylelint fix:prettier",
    "fix:eslint": "eslint --cache --cache-location node_modules --fix .",
    "fix:stylelint": "stylelint --fix '**/*.{css,scss,astro}'",
    "fix:prettier": "prettier --write --cache . '**/*.{css,scss,astro}'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "10.2.6",
    "@astrojs/mdx": "3.0.0",
    "@astrojs/partytown": "2.1.0",
    "@astrojs/sitemap": "3.1.4",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/ts-plugin": "1.8.0",
    "@builder.io/qwik": "1.5.4",
    "@iconify-json/devicon": "1.1.41",
    "@iconify-json/material-symbols": "1.1.79",
    "@iconify-json/material-symbols-light": "1.1.19",
    "@iconify/tailwind": "1.0.1",
    "@qwikdev/astro": "0.5.15",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgx/core": "1.0.1",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/typography": "0.5.13",
    "@totto/lib": "npm:@jsr/totto__lib@0.2.1",
    "@types/fs-extra": "11.0.4",
    "astro": "4.8.5",
    "clsx": "2.1.1",
    "cva": "beta",
    "daisyui": "4.11.1",
    "htmlrewriter": "0.0.7",
    "js-cookie": "3.0.5",
    "jsr": "0.12.4",
    "license-checker-rseidelsohn": "4.3.0",
    "npm-run-all2": "6.1.2",
    "rehype-external-links": "3.0.0",
    "remeda": "1.61.0",
    "sass": "1.77.1",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "unplugin-icons": "0.19.0",
    "valibot": "0.30.0",
    "vite": "5.2.11"
  },
  "devDependencies": {
    "@astrojs/check": "0.7.0",
    "@eslint/js": "9.2.0",
    "@types/js-cookie": "3.0.6",
    "bun-types": "1.1.8",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "1.1.2",
    "eslint-plugin-tailwindcss": "3.15.2",
    "postcss-html": "1.7.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "stylelint": "16.5.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0",
    "wrangler": "3.55.0"
  },
  "overrides": {
    "astro-integration-kit": "0.3.1"
  }
}
